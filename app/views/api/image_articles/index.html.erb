<h1>Listing image_articles</h1>

<table>
  <thead>
    <tr>
      <th>Articles</th>
      <th>Images</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @image_articles.each do |image_article| %>
      <tr>
        <td><%= image_article.articles %></td>
        <td><%= image_article.images %></td>
        <td><%= link_to 'Show', image_article %></td>
        <td><%= link_to 'Edit', edit_image_article_path(image_article) %></td>
        <td><%= link_to 'Destroy', image_article, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Image article', new_image_article_path %>
